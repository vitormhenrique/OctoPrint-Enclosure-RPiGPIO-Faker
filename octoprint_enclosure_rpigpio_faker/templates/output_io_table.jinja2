<table class="table table-striped table-hover table-condensed table-hover" id="settings_enclosure_io" style="">
    <thead>
      <tr>
        <th class="">Id</th>
        <th class="">Label</th>
        <th class="">Pin</th>
        <th class="">Active Low</th>
        <th class="">{{ _('Action') }}</th>
      </tr>
    </thead>
    <tbody data-bind="foreach: $root.rpi_output_ios">
      <tr data-bind="attr: {title: name}">
        <td class="" data-bind="text: index_id"></td>
        <td class="" data-bind="text: label"></td>
        <td class="" data-bind="text: gpio_pin"></td>
        <td class="" data-bind="text: active_low"></td>
        <td class="">
          <a href="#" class="fa fa-pencil" title="{{ _('Edit Output')|edq }}" data-bind="click: function() { $root.showOutputIODialog($data); }"></a>&nbsp;|&nbsp;
          <a href="#" class="fa fa-trash-o" title="{{ _('Delete Output')|edq }}" data-bind="click: function() { $root.removeOutputIO($data); }"></a>
      </td>
      </tr>
    </tbody>
  </table>
  
  <div class="control-group">
    <button class="btn pull-right" data-bind="click: function() { $root.showOutputIODialog(); }">Add
      Outputs...</button>
  </div>